{
	"version": "0.2.0",
	"configurations": [

		{
			"name": "Launch",
			"type": "node2",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/jasmine/bin/jasmine.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"console": "integratedTerminal",
			"sourceMaps": true,
			"outFiles": []
		},
		{
			"name": "Attach to chrome",
			"type": "chrome",
			"request": "attach",
			"port": 9221
		}

	]
}